<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:TSControls="clr-namespace:TripSketch.Controls;assembly=TripSketch"
             x:Class="TripSketch.Views.EventsGridView">
  <TSControls:EventsGrid Start="{Binding Start}"
                         End="{Binding End}"
                         Events="{Binding Events}"
                         AddEventCommand="{Binding AddEventCommand}"
                         EditEventCommand="{Binding EditEventCommand}"/>
</ContentView>